{
  "paths": {
    "log_dir": "logs",
    "kismet_logs": ".",
    "ignore_lists": {
      "mac": "mac_ignore.json",
      "ssid": "ssid_ignore.json"
    }
  },
  "api_keys": {
    "wigle": ""
  },
  "search": {
    "lat_min": null,
    "lat_max": null,
    "lon_min": null,
    "lon_max": null
  },
  "timing": {
    "check_interval": 60,
    "list_update_interval": 5,
    "time_windows": {
        "recent": 5,
        "medium": 10,
        "old": 15,
        "oldest": 20
    }
  },
  "kismet_health": {
    "enabled": true,
    "check_interval_cycles": 5,
    "data_freshness_threshold_minutes": 5,
    "auto_restart": false,
    "max_restart_attempts": 3,
    "startup_script": "./start_kismet_clean.sh",
    "interface": "wlan0mon"
  }
}
